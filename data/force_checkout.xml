<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data noupdate="1">

        <record id="hr_china_force_checkout" model="ir.cron">
            <field name="name">Attendance Force Checkout</field>
            <field name="active" eval="True" />
            <field name="user_id" ref="base.user_root" />
            <field name="interval_number">1</field>
            <field name="interval_type">days</field>
            <field name="numbercall">-1</field>
            <field name="nextcall" eval="(DateTime.now()).strftime('%Y-%m-%d 23:59:00')" />
            <field name="doall" eval="False" />
            <field name="model" eval="'hr_china.attendance'" />
            <field name="function" eval="'force_checkout_attendance'" />
            <field name="priority">5</field>
        </record>

    </data>
</odoo>